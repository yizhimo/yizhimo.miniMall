/* pages/profile/childCpns/v-user/v-user.wxss */
.user {
  background-color: #ff5777;
  display: flex;
  color: #fff;
  font-size: 28rpx; 
  padding: 20rpx 0 30rpx 0;
}
.enter {
  width: 160rpx;
  text-align: center;
}
.enter image{
  width: 90%;
  height: 90%;
  margin-top: 4rpx;
}
.right {
  width: 100rpx;
  text-align: center;
}
.right image{
  width: 50%;
  height: 50%;
  margin-top: 30%;
}
.phone {
  flex: 1;
  padding-left: 2%;
  margin-top: 10rpx;
}
.phone span{
  font-size: 24rpx;
  vertical-align: middle;
}
.phone image{
  width: 44rpx;
  height: 44rpx;
  vertical-align: middle;
}
.phoneTop {
  margin-left: 6rpx;
}
.phoneButton {
  margin: 10rpx -6rpx 0 -4rpx;
}